{
    "name": "rslanzi/nova-translatable",
    "description": "A Laravel Nova field for astrotomic/laravel-translatable.",
    "keywords": [
        "laravel",
        "nova",
        "translatable",
        "ckeditor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Riccardo Slanzi",
            "email": "rslanzi@gmail.com",
             "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/nova": "^2.9 || ^3.0",
        "astrotomic/laravel-translatable": "^11.5",
        "waynestate/nova-ckeditor4-field": "^0.6.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "friendsofphp/php-cs-fixer": "^2.17",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.17",
        "spatie/ray": "^1.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rslanzi\\NovaTranslatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rslanzi\\NovaTranslatable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rslanzi\\NovaTranslatable\\FieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
