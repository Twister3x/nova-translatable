{
    "name": "twister3x/nova-translatable",
    "description": "A Laravel Nova field for astrotomic/laravel-translatable.",
    "keywords": [
        "laravel",
        "nova",
        "translatable",
        "ckeditor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Riccardo Slanzi",
            "email": "rslanzi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "astrotomic/laravel-translatable": "^11.5",
        "waynestate/nova-ckeditor4-field": "^0.6.0"
    },
    "require-dev": {
        "laravel/nova": "^2.9 || ^3.0 || ^4.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rslanzi\\NovaTranslatable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rslanzi\\NovaTranslatable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
